{{#each contracts}}
import {{namespaceCamel}}{{namePascal}}EventContract_{{version}}{{#if isLocal}}, { {{namePascal}}EventContract as {{namespacePascal}}{{namePascal}}EventContract_{{version}}  }{{/if}} from '#spruce/events/{{ namespaceCamel }}/{{ nameCamel }}.{{ version }}.contract'
{{/each}}

export default [
    {{#each contracts}}
    {{namespaceCamel}}{{namePascal}}EventContract_{{version}},
    {{/each}}
]


declare module '@sprucelabs/mercury-types/build/types/mercury.types' {
    interface SkillEventSignatures {
    {{#each contracts}}
    {{#if isLocal}}
        {{#each eventSignatures}}
        '{{@key}}': {{../namespacePascal}}{{../namePascal}}EventContract_{{../version}},
        {{/each}}
    {{/if}}
    {{/each}}
    }
}