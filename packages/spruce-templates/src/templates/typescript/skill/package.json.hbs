	"scripts": {
		"#build": "npm run clean && npm run build:server && npm run build:config && npm run build:interface",
		"build": "npm run clean && npm run build:server",
		"build:next": "next build web && next export web",
		"build:interface": "NODE_CONFIG_DIR=./build/config next build interface",
		"build:server": "tsc --project tsconfig.json | true",
		"#build:config": "cp -r icon build && NODE_CONFIG_DIR=./build/config node ./bin/buildConfigs.js",
		"build:config": "NODE_CONFIG_DIR=./build/config node ./bin/buildConfigs.js",
		"clean": "rm -rf build/",
		"start": "NODE_CONFIG_DIR=./build/config node ./build/server/server.js",
		"local": "npm run kill-inspect && NODE_CONFIG_DIR=./.spruce/config tsnd --ignore-watch interface/.next --ignore-watch node_modules  --ignore-watch generated.ts --project tsconfig.json --inspect=4080 --respawn --transpileOnly ./.spruce/server/server.ts",
		"kill-inspect": "kill-port 4080",
		"lint": "eslint --max-warnings=0 --ext=.js,.jsx,.ts,.tsx . && tsc -p . --noEmit",
		"tsc": "tsc",
		"release": "semantic-release"
	},
