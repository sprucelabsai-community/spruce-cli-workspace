import { TemplateRenderAs } from '@sprucelabs/schema'

{{#each imports}}
// @ts-ignore
import * as {{importAs}} from '{{package}}'
{{/each}}

{{!-- import FieldType from '#spruce/schemas/fields/fieldTypeEnum' --}}

// @ts-ignore
const generateType = (renderAs: TemplateRenderAs, importAs: string) => {
	return 'string'
}

const valueTypes = {
	{{#each schemaTemplatesByNamespace}}
	{{@key}}: {
		{{#each this}} 
		{{id}}: {
			{{#each definition.fields}}
			"{{@key}}": {
				{{#each ../../../rendersAs}}
				{{this}}: {{{ valueTypeGenerator ../this this }}},
				{{/each}}
			},
			{{/each}}
		},
		{{/each}}
	},
	{{/each}}
}

export default valueTypes
