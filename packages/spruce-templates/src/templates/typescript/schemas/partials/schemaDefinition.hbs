id: '{{{escape schema.id}}}',
name: '{{{escape schema.name}}}',
description: '{{{escape schema.description}}}',

{{#if schema.dynamicKeySignature}}dynamicKeySignature: { 
    {{> fieldDefinition field=schema.dynamicKeySignature renderAs=renderAs}} 
}{{/if}}
{{#if schema.fields}}
    fields: {
        {{#each schema.fields}}
            /** {{{label}}}.{{#if hint}} {{{hint}}}{{/if}} */
            '{{@key}}': {
                {{> fieldDefinition field=this renderAs=../renderAs  }}
            },
        {{/each}}
    }
{{/if}}
