id: '{{{escape schema.id}}}',{{#if schema.version}}
version: '{{{escape schema.version}}}',{{/if}}{{#if namespace}}
namespace: '{{{escape namespace}}}',{{/if}}
name: '{{{escape schema.name}}}'{{#if schema.description}},
description: '{{{escape schema.description}}}'{{/if}}{{#if schema.dynamicFieldSignature}},
dynamicFieldSignature: { 
    {{> fieldDefinitionBody namespace=namespace nameCamel=nameCamel schema=schema fieldName="__dynamicFieldSignature" field=schema.dynamicFieldSignature renderAs=renderAs}} 
}{{/if}}{{#if schema.fields}},
    fields: {
        {{#each schema.fields}}
            /** {{{label}}}.{{#if hint}} {{{hint}}}{{/if}} */
            '{{@key}}': {
                {{> fieldDefinitionBody namespace=../namespace nameCamel=../nameCamel schema=../schema fieldName=@key field=this renderAs=../renderAs  }}
            },
        {{/each}}
    }
{{/if}}
