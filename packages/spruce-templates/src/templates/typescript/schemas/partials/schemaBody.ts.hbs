id: '{{{escape schema.id}}}',
name: '{{{escape schema.name}}}'{{#if schema.description}},
description: '{{{escape schema.description}}}'{{/if}}{{#if schema.dynamicKeySignature}},
dynamicKeySignature: { 
    {{> fieldDefinitionBody namespace=namespace schema=schema fieldName="__dynamic" field=schema.dynamicKeySignature renderAs=renderAs}} 
}{{/if}}{{#if schema.fields}},
    fields: {
        {{#each schema.fields}}
            /** {{{label}}}.{{#if hint}} {{{hint}}}{{/if}} */
            '{{@key}}': {
                {{> fieldDefinitionBody namespace=../namespace schema=../schema fieldName=@key field=this renderAs=../renderAs  }}
            },
        {{/each}}
    }
{{/if}}
