{
	"name": "@sprucelabs/spruce-cli",
	"description": "Command line interface for building Spruce skills",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"version": "13.1.2",
	"bin": {
		"spruce": "./build/index.js"
	},
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"files": [
		"**/*"
	],
	"keywords": [
		"node",
		"components",
		"sprucebot",
		"spruce",
		"skills",
		"sprucelabs"
	],
	"homepage": "https://github.com/sprucelabsai/spruce-cli-workspace",
	"bugs": {
		"url": "https://github.com/sprucelabsai/spruce-cli-workspace/issues"
	},
	"scripts": {
		"lint": "eslint '**/*.ts' && yarn lint.tsc",
		"lint.tsc": "tsc --noEmit && echo 'spruce-cli PASS'",
		"lint.fix": "eslint --fix '**/*.ts'",
		"rebuild": "cd ../ && yarn rebuild",
		"build": "yarn build.babel && yarn build.types && yarn build.resolve-paths",
		"build.types": "tsc --emitDeclarationOnly && echo PASS",
		"build.babel": "babel src --out-dir build --extensions \".ts,.tsx\" --root-mode upward --source-maps --copy-files",
		"build.babel.debug": "node --inspect-brk=5200 ../../node_modules/.bin/babel src --out-dir build --extensions \".ts,.tsx\" --root-mode upward --source-maps --copy-files",
		"build.resolve-paths": "resolve-path-aliases --target build --patterns **/*.js,**/*.d.ts",
		"cache.tests": "node build/tests/buildTestCache",
		"clean": "rm -rf build/",
		"clean.all": "rm -rf build/ && rm -rf node_modules/ && yarn clean.cache",
		"clean.cache": "rm -rf src/.spruce/tmp && node build/tests/cleanTestCache && echo '******** TEST CACHE CLEARED ********'",
		"clean.cache.disabled": "echo 'CLEAN.CACHE DISABLED'",
		"release": "semantic-release",
		"start": "DEBUG=@sprucelabs/cli~debug node . $COMMANDS",
		"local": "DEBUG=@sprucelabs/cli~debug node --inspect=5200 . $COMMANDS ; echo 'DONE'",
		"test": "yarn cache.tests && jest",
		"watch.tests": "yarn cache.tests && jest --watch",
		"watch.build": "concurrently 'yarn run build.babel --watch' \"chokidar 'src/**/*' --ignore '.*/tmp/.*' -c 'clear && yarn build.types && yarn build.resolve-paths'\" \"chokidar '../spruce-templates/src/**/*.hbs' -c 'yarn clean.cache && yarn build.types'\"",
		"watch.rebuild": "cd ../ && yarn watch.rebuild",
		"watch.lint": "chokidar 'src/**/*' '../spruce-templates/src/**' -c 'yarn lint.tsc'",
		"arkit": "arkit -f index.ts -o docs/images/arkit.svg",
		"arkit.png": "arkit -f index.ts -o docs/images/arkit.png"
	},
	"dependencies": {
		"@sprucelabs/error": ">=4.0.1",
		"@sprucelabs/log": ">=10.2.295",
		"@sprucelabs/mercury-client": ">=14.6.0",
		"@sprucelabs/mercury-event-emitter": ">=14.5.15",
		"@sprucelabs/mercury-types": ">=15.0.2",
		"@sprucelabs/schema": ">=19.0.0",
		"@sprucelabs/spruce-core-schemas": ">=22.0.15",
		"@sprucelabs/spruce-skill-utils": ">=13.5.1",
		"fs-extra": "^9.0.1",
		"gifwrap": "SimenB/gifwrap#patch-1"
	},
	"devDependencies": {
		"@sprucelabs/babel-plugin-schema": ">=3.1.2",
		"@sprucelabs/jest-sheets-reporter": ">=1.2.5",
		"@sprucelabs/test": ">=7.3.0",
		"@sprucelabs/test-utils": ">=3.0.2"
	},
