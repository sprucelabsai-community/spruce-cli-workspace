{
	"name": "@sprucelabs/spruce-cli",
	"publishConfig": {
		"access": "public"
	},
	"version": "10.5.1",
	"bin": {
		"spruce": "./index.js"
	},
	"main": "./index.js",
	"types": "./build/index.d.ts",
	"files": [
		"**/*"
	],
	"description": "Command line interface for building Spruce skills",
	"keywords": [
		"node",
		"components",
		"sprucebot",
		"spruce",
		"skills",
		"sprucelabs"
	],
	"homepage": "https://github.com/sprucelabsai/spruce-cli-workspace",
	"bugs": {
		"url": "https://github.com/sprucelabsai/spruce-cli-workspace/issues"
	},
	"scripts": {
		"build": "npm run clean && npm run build.node",
		"build.node": "tsc",
		"watch": "chokidar '.spruce/**' 'src/**/*' 'index.ts' '../spruce-templates/src/templates/**/*' -c 'npm run build'",
		"clean": "rm -rf build/",
		"lint": "eslint '**/*.ts' && npm run lint.tsc",
		"lint.tsc": "tsc -p .",
		"lint.all": "eslint '**/*.ts' '.spruce/**/*.ts' && npm run lint.tsc",
		"lint.fix": "eslint --fix '**/*.ts'",
		"release": "semantic-release",
		"start": "echo $COMMANDS && DEBUG=@sprucelabs/cli~debug node . $COMMANDS",
		"local": "echo $COMMANDS && DEBUG=@sprucelabs/cli~debug node --inspect=5200 . $COMMANDS",
		"test": "jest",
		"test.watch": "jest --watch"
	},
	"dependencies": {
		"@sprucelabs/error": "^1.2.0",
		"@sprucelabs/log": "^10.2.1",
		"@sprucelabs/mercury": "10.3.0-prerelease-mock.2",
		"@sprucelabs/schema": "^4.0.6",
		"@sprucelabs/spruce-templates": "^10.5.1",
		"@types/promise.allsettled": "^1.0.3",
		"@types/uuid": "^3",
		"cfonts": "^2.8.1",
		"chokidar": "^3.3.1",
		"commander": "^4.1.1",
		"dotenv": "^8.2.0",
		"emphasize": "^3.0.0",
		"eslint": "^5",
		"fs-extra": "^8.1.0",
		"globby": "^11.0.0",
		"inflection": "^1.12.0",
		"inquirer": "^7.0.6",
		"inquirer-file-tree-selection-prompt": "^1.0.5",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.15",
		"minimatch": "^3.0.4",
		"ora": "^4.0.3",
		"promise.allsettled": "^1.0.2",
		"rimraf": "^3.0.2",
		"semver": "^7.3.2",
		"slug": "^2.1.1",
		"string-argv": "^0.3.1",
		"tsutils": "^3.17.1",
		"typescript": "^3.8.3",
		"uuid": "^3",
		"vm2": "^3.9.1"
	},
	"devDependencies": {
		"@sprucelabs/test": "^5.2.4",
		"@types/eslint": "^6.8.0",
		"@types/fs-extra": "^8.1.0",
		"@types/google-spreadsheet": "^3.0.0",
		"@types/inflection": "^1.5.28",
		"@types/inquirer": "^6.5.0",
		"@types/jsonwebtoken": "^8.3.8",
		"@types/lodash": "^4.14.149",
		"@types/mkdirp": "^1.0.0",
		"@types/node": "^13.7.7",
		"@types/rimraf": "^3.0.0",
		"@types/semver": "^7.1.0",
		"@types/sha1": "^1.1.2",
		"@types/slug": "^0.9.1",
		"@types/superagent": "^4.1.7",
		"chokidar-cli": "^2.1.0",
		"concurrently": "^5.0.2",
		"conventional-changelog-sprucelabs": "^1.1.0",
		"dotenv": "^8.2.0",
		"google-spreadsheet": "^3.0.11",
		"jest": "^26.0.1",
		"jest-reporters": "^0.0.2",
		"mkdirp": "^1.0.3",
		"prettier": "^1.13",
		"tsconfig-paths": "^3.9.0",
		"ts-jest": "^26.1.0",
		"ts-node": "^8.10.2"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			],
			"@babel/preset-typescript"
		]
	},
	"jest": {
		"preset": "ts-jest",
		"maxWorkers": 4,
		"testTimeout": 60000,
		"testEnvironment": "node",
		"testPathIgnorePatterns": [
			"<rootDir>/tmp/",
			"<rootDir>/build/",
			"<rootDir>/node_modules/",
			"<rootDir>/src/__tests__/testDirsAndFiles/"
		],
		"testMatch": [
			"**/__tests__/**/*.test.[jt]s?(x)"
		],
		"moduleNameMapper": {
			"^#spruce/schemas/fields/(.*)": "<rootDir>/../../node_modules/@sprucelabs/schema/build/.spruce/schemas/fields/$1",
			"^#spruce/(.*)$": "<rootDir>/src/.spruce/$1"
		},
		"reporters": [
			"default",
			[
				"<rootDir>/build/jest/SheetsReporter.js",
				{
					"adapterFilepath": "build/jest/GoogleSpreadsheetAdapter",
					"sheetId": "1DoWM7mYgGDa-PJXptLomrZmbIBZiuxS1YiAsRXU2Gm0",
					"worksheetId": 843037832,
					"testMap": {
						"canBootTheCli": "C4",
						"getsGoodHealthCheckAndNothingElse": "C5",
						"canGetLatestVersionBasedOnDir": "C16",
						"builderFileValidates": "C7",
						"schemaPassesHealthCheck": "C6"
					}
				}
			]
		]
	}
}
