{
	"name": "@sprucelabs/spruce-cli",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"version": "10.20.44",
	"bin": {
		"spruce": "./build/index.js"
	},
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"files": [
		"**/*"
	],
	"description": "Command line interface for building Spruce skills",
	"keywords": [
		"node",
		"components",
		"sprucebot",
		"spruce",
		"skills",
		"sprucelabs"
	],
	"homepage": "https://github.com/sprucelabsai/spruce-cli-workspace",
	"bugs": {
		"url": "https://github.com/sprucelabsai/spruce-cli-workspace/issues"
	},
	"scripts": {
		"lint": "eslint '**/*.ts' && yarn lint.tsc",
		"lint.tsc": "tsc --noEmit && echo 'spruce-cli PASS'",
		"lint.fix": "eslint --fix '**/*.ts'",
		"lint.watch": "chokidar 'src/**/*' '../spruce-templates/src/**' -c 'yarn lint.tsc'",
		"rebuild": "cd ../ && yarn rebuild",
		"rebuild.watch": "cd ../ && yarn rebuild.watch",
		"build": "yarn build.babel && yarn build.types",
		"build.types": "tsc --emitDeclarationOnly && echo PASS",
		"build.babel": "babel src --out-dir build --extensions \".ts,.tsx\" --root-mode upward --source-maps --copy-files",
		"build.babel.debug": "node --inspect-brk=5200 ../../node_modules/.bin/babel src --out-dir build --extensions \".ts,.tsx\" --root-mode upward --source-maps --copy-files",
		"build.watch": "concurrently 'yarn run build.babel --watch' \"chokidar 'src/**/*' --ignore '.*/tmp/.*' -c 'yarn build.types'\" \"chokidar '../spruce-templates/src/**/*' -c 'yarn clean.cache && yarn build.types'\"",
		"clean": "rm -rf build/",
		"clean.all": "rm -rf build/ && rm -rf node_modules/ && yarn run clean.cache",
		"clean.cache": "rm -rf src/.spruce/tmp",
		"release": "semantic-release",
		"start": "echo $COMMANDS && DEBUG=@sprucelabs/cli~debug node . $COMMANDS",
		"local": "echo $COMMANDS && DEBUG=@sprucelabs/cli~debug node --inspect=5200 . $COMMANDS",
		"test": "jest",
		"test.watch": "jest --watch"
	},
	"dependencies": {
		"@babel/runtime": "^7.11.2",
		"@sprucelabs/error": "^2.2.111",
		"@sprucelabs/log": "^10.2.154",
		"@sprucelabs/mercury": "^10.5.72",
		"@sprucelabs/schema": "^7.1.4",
		"@sprucelabs/spruce-skill-utils": "^10.20.44",
		"@sprucelabs/spruce-templates": "^10.20.44",
		"blessed": "^0.1.81",
		"blessed-contrib": "^4.8.20",
		"cfonts": "^2.8.6",
		"chokidar": "^3.4.2",
		"cli-table3": "^0.6.0",
		"commander": "^6.0.0",
		"core-js": "^3.6.5",
		"emphasize": "^4.0.0",
		"fs-extra": "^9.0.1",
		"globby": "^11.0.1",
		"inflection": "^1.12.0",
		"inquirer": "^7.3.3",
		"inquirer-file-tree-selection-prompt": "^1.0.6",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.20",
		"minimatch": "^3.0.4",
		"ora": "^5.0.0",
		"semver": "^7.3.2",
		"string-argv": "^0.3.1",
		"tsutils": "^3.17.1",
		"typescript": "^4.0.2",
		"uuid": "^8.3.0"
	},
	"devDependencies": {
		"@sprucelabs/jest-sheets-reporter": "^1.1.2",
		"@sprucelabs/test": "^6.0.64",
		"@types/blessed": "^0.1.17",
		"@types/eslint": "^7.2.1",
		"@types/fs-extra": "^9.0.1",
		"@types/inflection": "^1.5.28",
		"@types/inquirer": "^7.3.1",
		"@types/jsonwebtoken": "^8.5.0",
