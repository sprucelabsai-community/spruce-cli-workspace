{
	"name": "@sprucelabs/spruce-cli",
	"publishConfig": {
		"access": "public"
	},
	"version": "10.7.23",
	"bin": {
		"spruce": "./build/index.js"
	},
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"files": [
		"**/*"
	],
	"description": "Command line interface for building Spruce skills",
	"keywords": [
		"node",
		"components",
		"sprucebot",
		"spruce",
		"skills",
		"sprucelabs"
	],
	"homepage": "https://github.com/sprucelabsai/spruce-cli-workspace",
	"bugs": {
		"url": "https://github.com/sprucelabsai/spruce-cli-workspace/issues"
	},
	"scripts": {
		"build": "npm run build.babel && npm run build.types",
		"build.tsc": "tsc",
		"build.types": "tsc --emitDeclarationOnly",
		"build.watch": "babel src --out-dir build --extensions \".ts,.tsx\" --root-mode upward --source-maps --copy-files --watch",
		"build.babel": "babel src --out-dir build --extensions \".ts,.tsx\" --root-mode upward --source-maps --copy-files",
		"clean": "rm -rf build/",
		"clean.all": "rm -rf build/ && rm -rf node_modules/",
		"lint": "eslint '**/*.ts' && npm run lint.tsc",
		"lint.tsc": "tsc --noEmit && echo 'PASS'",
		"lint.fix": "eslint --fix '**/*.ts'",
		"lint.watch": "chokidar 'src/**/*' '../spruce-templates/src/**' -c 'npm run lint.tsc'",
		"release": "semantic-release",
		"start": "echo $COMMANDS && DEBUG=@sprucelabs/cli~debug node . $COMMANDS",
		"local": "echo $COMMANDS && DEBUG=@sprucelabs/cli~debug node --inspect=5200 . $COMMANDS",
		"test": "jest",
		"test.watch": "jest --watch"
	},
	"dependencies": {
		"@babel/runtime": "^7.10.5",
		"@sprucelabs/error": "^1.3.42",
		"@sprucelabs/log": "^10.2.45",
		"@sprucelabs/mercury": "^10.5.41",
		"@sprucelabs/schema": "^4.0.51",
		"@sprucelabs/spruce-templates": "^10.7.23",
		"@types/promise.allsettled": "^1.0.3",
		"@types/uuid": "^8.0.0",
		"blessed": "^0.1.81",
		"blessed-contrib": "^4.8.20",
		"cfonts": "^2.8.6",
		"chokidar": "^3.4.1",
		"commander": "^6.0.0",
		"core-js": "^3.6.5",
		"emphasize": "^4.0.0",
		"fs-extra": "^9.0.1",
		"globby": "^11.0.1",
		"inflection": "^1.12.0",
		"inquirer": "^7.3.3",
		"inquirer-file-tree-selection-prompt": "^1.0.6",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.19",
		"minimatch": "^3.0.4",
		"ora": "^4.0.5",
		"promise.allsettled": "^1.0.2",
		"regenerator-runtime": "^0.13.7",
		"semver": "^7.3.2",
		"slug": "^3.3.2",
		"string-argv": "^0.3.1",
		"tsutils": "^3.17.1",
		"typescript": "^3.9.7",
		"uuid": "^8.2.0",
		"vm2": "^3.9.2"
	},
	"devDependencies": {
		"@sprucelabs/test": "^5.3.48",
		"@types/blessed": "^0.1.17",
		"@types/eslint": "^7.2.0",
		"@types/fs-extra": "^9.0.1",
		"@types/google-spreadsheet": "^3.0.0",
		"@types/inflection": "^1.5.28",
		"@types/inquirer": "^7.3.0",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/lodash": "^4.14.158",
		"@types/mkdirp": "^1.0.1",
		"@types/node": "^14.0.25",
		"@types/rimraf": "^3.0.0",
		"@types/semver": "^7.3.1",
		"@types/sha1": "^1.1.2",
		"@types/slug": "^0.9.1",
		"@types/superagent": "^4.1.8",
		"arkit": "^1.6.4",
		"chokidar-cli": "^2.1.0",
		"concurrently": "^5.2.0",
		"conventional-changelog-sprucelabs": "^1.1.1",
		"dotenv": "^8.2.0",
		"eslint": "^7.5.0",
		"google-spreadsheet": "^3.0.11",
		"jest": "^26.1.0",
		"jest-reporters": "^0.0.2",
		"mkdirp": "^1.0.4",
		"prettier": "^2.0.5",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.1.3",
		"ts-node": "^8.10.2",
		"tsconfig-paths": "^3.9.0"
	},
	"jest": {
		"maxWorkers": 4,
		"testTimeout": 120000,
		"testEnvironment": "node",
		"testPathIgnorePatterns": [
			"<rootDir>/tmp/",
			"<rootDir>/src/",
			"<rootDir>/node_modules/",
			"<rootDir>/build/__tests__/testDirsAndFiles/"
		],
		"testMatch": [
			"**/__tests__/**/*.test.js?(x)"
		],
		"moduleNameMapper": {
			"^#spruce/schemas/fields/(.*)": "<rootDir>/node_modules/@sprucelabs/schema/build/.spruce/schemas/fields/$1",
			"^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
		},
		"reporters": [
			"default",
			[
				"<rootDir>/build/sheetsReporter/SheetsReporter.js",
				{
					"sheetId": "1DoWM7mYgGDa-PJXptLomrZmbIBZiuxS1YiAsRXU2Gm0",
					"worksheetId": 843037832,
					"testMap": {
						"canBootTheCli": "C4",
						"getsGoodHealthCheckAndNothingElse": "C5",
						"schemaPassesHealthCheck": "C6",
						"builderFileValidates": "C7",
						"schemasStayInSyncWhenDefinitionsAreDeleted": "C8",
						"installsSchemasIfNotInstalled": "C9",
						"buildCreatesValidDefinitionAndOptionsFile": "C11",
						"errorsStayInSyncWhenDefinitionsAreDeleted": "C12",
						"canGetLatestVersionBasedOnDir": "C16"
					}
				}
			]
		]
	},
	"gitHead": "94ce410c05180cdfa9b6ca4b9e47061d52ac4e71"
}
