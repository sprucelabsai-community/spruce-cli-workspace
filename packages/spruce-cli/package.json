		"@types/uuid": "^8.3.0",
		"arkit": "^1.6.4",
		"chokidar-cli": "^2.1.0",
		"concurrently": "^5.3.0",
		"conventional-changelog-sprucelabs": "^1.1.1",
		"dotenv": "^8.2.0",
		"eslint": "^7.7.0",
		"find-process": "^1.4.3",
		"jest": "^26.4.2",
		"jest-reporters": "^0.0.2",
