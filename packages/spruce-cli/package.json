	"bin": {
		"spruce": "./build/index.js"
	},
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"files": [
		"**/*"
	],
	"keywords": [
		"node",
		"components",
		"sprucebot",
		"spruce",
		"skills",
		"sprucelabs"
