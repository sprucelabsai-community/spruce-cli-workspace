      },
      "pattern": "**/!(*.test).ts"
    },
    {
      "lookupDir": {
        "path": "src/services"
      },
      "destination": {
        "name": "services.ts",
        "path": ".spruce/autoloaders",
        "type": "application/typescript",
        "ext": ".ts"
      },
      "pattern": "**/!(*.test).ts"
    },
    {
      "lookupDir": {
        "path": "src/features"
      },
      "destination": {
        "name": "features.ts",
        "path": ".spruce/autoloaders",
        "type": "application/typescript",
        "ext": ".ts"
      },
      "pattern": "**/!(*.test).ts"
    },
    {
      "lookupDir": {
        "path": "src/generators"
      },
